{
    "name": "consumerr/php",
    "type": "library",
    "description": "Consumerr error and exception logger for PHP",
    "license": ["MIT"],
    "keywords": [
        "consumerr",
        "php",
        "debug",
        "logging"
    ],
    "homepage": "http://consumerr.io",
    "require": {
        "php": ">= 5.3.0",
        "ext-json": "*"
    },
    "require-dev": {
        "nette/tester": "@stable",
        "jakub-onderka/php-parallel-lint": "~0.8"
    },
    "suggest": {
        "ext-curl": "Allows sending data via cURL"
    },
    "support": {
        "email": "support@consumerr.io",
        "issues": "https://github.com/consumerr/php/issues"
    },
    "autoload": {
        "psr-0": {
            "Consumerr\\": "./src"
        },
        "classmap": [
            "src/Consumerr/exceptions.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "./tests"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    }
}